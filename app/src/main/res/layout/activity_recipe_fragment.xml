<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.RecipeFragment">

    <ImageView
        android:id="@+id/cookingtime"
        android:layout_width="86dp"
        android:layout_height="60dp"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="359dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/recipefavorite"
        android:layout_width="64dp"
        android:layout_height="44dp"
        app:srcCompat="@drawable/ic_fav_unselected"
        tools:layout_editor_absoluteX="316dp"
        tools:layout_editor_absoluteY="19dp" />

    <ImageView
        android:id="@+id/recipepagebacknav"
        android:layout_width="66dp"
        android:layout_height="46dp"
        app:srcCompat="@drawable/ic_back_arrow_12dp"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/recipepageheader"
        android:layout_width="207dp"
        android:layout_height="45dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="96dp"
        tools:layout_editor_absoluteY="18dp" />

    <ImageView
        android:id="@+id/recipepageimage"
        android:layout_width="359dp"
        android:layout_height="194dp"
        app:srcCompat="@drawable/signuppage"
        tools:layout_editor_absoluteX="12dp"
        tools:layout_editor_absoluteY="79dp" />

    <RatingBar
        android:id="@+id/reciperatingBar"
        android:layout_width="243dp"
        android:layout_height="46dp"
        tools:layout_editor_absoluteX="82dp"
        tools:layout_editor_absoluteY="280dp" />

    <ImageView
        android:id="@+id/recipedifficultylevel"
        android:layout_width="86dp"
        android:layout_height="60dp"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="359dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/recipecalories"
        android:layout_width="86dp"
        android:layout_height="60dp"
        tools:layout_editor_absoluteX="294dp"
        tools:layout_editor_absoluteY="359dp"
        tools:srcCompat="@tools:sample/avatars" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip"
        android:layout_width="108dp"
        android:layout_height="45dp"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="440dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/recipeingredients"
        android:layout_width="108dp"
        android:layout_height="45dp"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="440dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/recipeprocedure"
        android:layout_width="108dp"
        android:layout_height="45dp"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="440dp" />

    <TextView
        android:id="@+id/recipeingredientstext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="88dp"
        tools:layout_editor_absoluteY="452dp" />

    <TextView
        android:id="@+id/recipeproceduretext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="240dp"
        tools:layout_editor_absoluteY="452dp" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="363dp"
        android:layout_height="218dp"
        tools:layout_editor_absoluteX="11dp"
        tools:layout_editor_absoluteY="495dp" />

</androidx.constraintlayout.widget.ConstraintLayout>